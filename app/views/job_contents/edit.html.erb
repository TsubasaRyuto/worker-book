<% provide(:title, '発注内容変更') %>

<section class="job-content edit-page" id="update-job-content">
  <div class="container">
    <div class="row">
      <div class="form col-md-8 col-md-offset-2">
        <h1>発注内容変更</h1>
        <div class="row">
          <div class="col-md-8 col-md-offset-2">
            <%= form_for(@job_content, url: { controller: :job_contents, action: :update  })do |f| %>
              <%= render 'job_contents/commons/form', { f: f } %>
              <%= f.submit '変更を保存', class: "btn btn-primary" %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
